#include "confdefs.h"
#ifdef __cplusplus
extern "C" void exit(int);
#endif

main () {
    double e, u;
    /*
     *  Check the smallest value such that 1.0 + x != 1.0.
     *  For ANSI compilers this is DBL_EPSILON in float.h
     */
    u = 1.0;
    for(;;) {
       u *= 0.5;
       if ((1.0 + u) == 1.0) {
           break;
       }
    }
    e = u * 2.0;
    printf("%.17e\n", e);
    exit(0);
}
EOF
