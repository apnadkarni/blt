    conftest$ac_exeext conftest.$ac_ext
    if test "${found}" = "no" ; then
      # Then check locations based on prefix
      dir=$prefix/lib
      lib_spec="-L${dir} -lexpat "
      LIBS="${lib_spec} ${save_LIBS}"
      cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

